{
  "name": "api-healthy-mind",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:start": "gnome-terminal -e \"docker-compose -f ./tools/api.yml up --force-recreate\" && docker-compose -f tools/init.yml run --rm tests",
    "docker:stop": "docker-compose -f ./tools/api.yml down",
    "docker:migrate": "docker-compose -f tools/init.yml run --rm migrator",
    "docker:revert": "docker-compose -f tools/init.yml run --rm revert",
    "docker:tests": "docker-compose -f tools/init.yml run --rm tests",
    "start": "node dist/server.js",
    "build": "tsc",
    "start:server": "nodemon -w src --ext ts --exec ts-node --transpile-only src/server.ts",
    "deploy": "git push -f origin develop && git checkout main && git merge develop && git push -f origin main && git push -f heroku main && git checkout develop",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "ts-node ./node_modules/typeorm/cli migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli migration:revert",
    "lint": "eslint --ext .tsx --ext .ts src/ --fix",
    "format": "prettier --check src/",
    "prettier": "npx @btmills/prettier --write",
    "test": "npx jest"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^6.3.0",
    "generate-password": "^1.7.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "morgan-body": "^2.6.6",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "sinon": "^14.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.1",
    "tsyringe": "^4.6.0",
    "typeorm": "^0.2.39",
    "util": "^0.12.4",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@jest/globals": "^28.0.0",
    "@types/app-root-path": "^1.2.4",
